<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="419.0" prefWidth="728.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AdminViewUsersController">

    <children>
        <TableView fx:id="usersTableView" layoutX="47.0" layoutY="47.0" onMouseClicked="#onEdit" prefHeight="279.0" prefWidth="635.0">
            <columns>
                <TableColumn fx:id="userId" prefWidth="75.0" text="User ID" />
                <TableColumn fx:id="userName" prefWidth="75.0" text="Username" />
                <TableColumn fx:id="password" prefWidth="75.0" text="Password" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
      <Button layoutX="615.0" layoutY="14.0" mnemonicParsing="false" onAction="#toAdminPanel" text="Main Menu" />
      <Button layoutX="604.0" layoutY="362.0" mnemonicParsing="false" onAction="#deleting" text="Delete User" />
      <Label layoutX="321.0" layoutY="13.0" text="View Users">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
    </children>

</AnchorPane>
