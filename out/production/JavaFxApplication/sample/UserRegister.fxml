<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="444.0" prefWidth="626.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.UserRegisterController">
   <children>
      <Label layoutX="243.0" layoutY="45.0" text="Register">
         <font>
            <Font size="39.0" />
         </font>
      </Label>
      <TextField fx:id="username" layoutX="244.0" layoutY="117.0" promptText="username" />
      <PasswordField fx:id="empPassword" layoutX="244.0" layoutY="154.0" promptText="password" />
      <PasswordField fx:id="conpassword" layoutX="244.0" layoutY="189.0" promptText="confirm password" />
      <ComboBox fx:id="comboBox" layoutX="213.0" layoutY="229.0" prefHeight="25.0" prefWidth="201.0" promptText="Select Question" />
      <TextField fx:id="SecretAnswer" layoutX="244.0" layoutY="267.0" promptText="Secret Answer" />
      <Button fx:id="Register" layoutX="288.0" layoutY="305.0" mnemonicParsing="false" onAction="#registering" text="Register" />
      <Hyperlink fx:id="toLogin" layoutX="284.0" layoutY="346.0" onAction="#toLoginLink" text="Login page" />
   </children>
</AnchorPane>
