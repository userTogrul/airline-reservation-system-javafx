            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="flFrom" layoutX="67.0" layoutY="161.0" prefHeight="39.0" prefWidth="222.0" promptText="From" />
      <TextField fx:id="flTo" layoutX="327.0" layoutY="161.0" prefHeight="39.0" prefWidth="222.0" promptText="To" />
      <TextField fx:id="flDept" layoutX="67.0" layoutY="232.0" prefHeight="39.0" prefWidth="222.0" promptText="Departure" />
      <TextField fx:id="flRet" layoutX="327.0" layoutY="232.0" prefHeight="39.0" prefWidth="222.0" promptText="Return" />
      <Button layoutX="459.0" layoutY="330.0" mnemonicParsing="false" onAction="#toTicketDetails" text="Get the Ticket" />
      <Button layoutX="472.0" layoutY="28.0" mnemonicParsing="false" onAction="#toMainMenu" text="Main Menu" />
      <Label layoutX="67.0" layoutY="334.0" text="Price from:" />
      <Label fx:id="flEcoPrice" layoutX="140.0" layoutY="334.0" />
