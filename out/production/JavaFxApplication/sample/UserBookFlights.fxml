<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.UserBookFlightsController">
   <children>
      <Label layoutX="67.0" layoutY="75.0" prefHeight="30.0" prefWidth="222.0" text="Flights: book the flights">
